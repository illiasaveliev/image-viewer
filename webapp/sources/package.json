    "@types/node": "6.0.90",
    "codelyzer": "^5.2.2",
    "conventional-changelog-cli": "1.3.4",
    "husky": "0.13.3",
    "npm-run-all": "4.0.2",
    "rimraf": "2.6.1",
    "stylelint": "7.13.0",
